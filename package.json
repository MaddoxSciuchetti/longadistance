{
  "name": "livekit-elevenlabs-backend",
  "version": "1.0.0",
  "description": "Backend server for LiveKit signaling and ElevenLabs voice API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "worker": "node worker.js",
    "worker:dev": "node --watch worker.js",
    "worker:testroom": "node worker.js testroom",
    "kill-3000": "lsof -ti:3000 | xargs kill -9 || true",
    "start:dev": "npm run kill-3000 && npm run dev"
  },
  "keywords": [
    "livekit",
    "elevenlabs",
    "express",
    "voice",
    "signaling"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@livekit/rtc-node": "^0.13.18",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "livekit-server-sdk": "^2.0.5",
    "node-fetch": "^3.3.2",
    "undici": "^7.13.0",
    "wav": "^1.0.2",
    "ws": "^8.18.3"
  }
}
